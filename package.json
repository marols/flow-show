{
  "name": "flow-show",
  "version": "0.0.7",
  "description": "",
  "main": "bundle.js",
  "scripts": {
    "start": "node server.js",
    "watchify": "watchify --entry js/app.js --outfile bundle.js --verbose --debug",
    "browserify": "browserify -t browserify-ngannotate js/app.js > app.browserify.js",
    "uglify": "uglifyjs app.browserify.js --compress --mangle --screw-ie8 -o bundle.js",
    "dev": "npm run watchify | npm run start",
    "prod": "npm run browserify && npm run uglify && npm run start"
  },
  "author": "Martin Walter",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.5.9",
    "angular-animate": "^1.5.9",
    "angular-aria": "^1.5.9",
    "angular-bootstrap-npm": "^0.14.3",
    "angular-material": "^1.1.1",
    "angular-messages": "^1.5.9",
    "angular-ui-router": "^0.3.2"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "browserify-ngannotate": "^2.0.0",
    "connect": "^3.5.0",
    "serve-static": "^1.11.1",
    "uglify": "^0.1.5",
    "watchify": "^3.7.0"
  }
}

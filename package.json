{
  "name": "flow-show",
  "version": "0.0.7",
  "description": "",
  "main": "dist/bundle.js",
  "scripts": {
    "start": "node server.js",
    "watchify": "watchify --entry js/app.js --outfile dist/bundle.js --verbose --debug",
    "browserify": "browserify -t browserify-ngannotate js/app.js > dist/app.browserify.js",
    "uglify": "uglifyjs dist/app.browserify.js --compress --mangle --screw-ie8 -o dist/bundle.js",
    "dev": "mkdir -p dist && npm run watchify | npm run start",
    "prod": "mkdir -p dist && npm run browserify && npm run uglify && npm run start"
  },
  "author": "Martin Walter",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.5.9",
    "angular-animate": "^1.5.9",
    "angular-aria": "^1.5.9",
    "angular-bootstrap-npm": "^0.14.3",
    "angular-material": "^1.1.1",
    "angular-messages": "^1.5.9",
    "angular-ui-router": "^0.3.2",
    "mdi": "^1.7.22"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "browserify-ngannotate": "^2.0.0",
    "connect": "^3.5.0",
    "serve-static": "^1.11.1",
    "uglify": "^0.1.5",
    "watchify": "^3.7.0"
  }
}
